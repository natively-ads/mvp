{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [".env"],
  "pipeline": {
    "generate": {
        "dependsOn": ["^generate"],
        "outputs": []
    },
    "clean": {
        "outputs": [],
        "cache": false
      },
    "build": {
        "dependsOn": ["^build"],
        "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
        "dependsOn": ["^lint"]
    },
    "dev": {
        "cache": true,
        "persistent": true
    }
  }
}
